<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.image.*?>

<StackPane prefWidth="231.0" xmlns:fx="http://javafx.com/fxml" fx:controller="Controllers.RegisterController">
  <children>
    <StackPane>
        <children>
            <StackPane prefHeight="-1.0" prefWidth="-1.0">
                  <children>
                    <Button id="loginButton" fx:id="logoutButton" defaultButton="true" mnemonicParsing="false" text="Logout" StackPane.alignment="TOP_RIGHT" />
                  </children>
            </StackPane>
        </children>
      <children>
        <VBox alignment="CENTER" spacing="10.0">     
          <children>
            <GridPane alignment="CENTER">
               <children>
                <Label fx:id="registerName" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                
                <Label text="Name:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="patientName" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                
                <Label text="Age:" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="patientAge" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                
                <Label text="Phone Number:" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <TextField fx:id="patientPhone" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                
                <Label text="Insurance Provider:" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                <TextField fx:id="patientProvider" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                
                <Label text="Social Security Number:" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                <TextField fx:id="patientSSN" GridPane.columnIndex="2" GridPane.rowIndex="5" />
                
                <Label text="Physician:" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                <TextField fx:id="patientPhysician" GridPane.columnIndex="2" GridPane.rowIndex="6" />
                
                <Label text="Physician Phone Number:" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                <TextField fx:id="patientPhysicianNum" GridPane.columnIndex="2" GridPane.rowIndex="7" />                   
                
                <Label text="Fever" GridPane.columnIndex="5" GridPane.rowIndex="1" />
                <fx:define>
                    <ToggleGroup fx:id="fever" />
                </fx:define>
                <RadioButton fx:id="yes" text="Yes" toggleGroup="$fever" GridPane.columnIndex="6" GridPane.rowIndex="1">
                    <HBox.margin>
                        <Insets right="3.0"/>
                    </HBox.margin>
                </RadioButton>

                <RadioButton fx:id="no" text="No" toggleGroup="$fever" GridPane.columnIndex="7" GridPane.rowIndex="1">
                    <HBox.margin>
                        <Insets right="30.0" bottom="10.0"/>
                    </HBox.margin>
                </RadioButton>  
                
                <Label fx:id="serverMessage"  GridPane.columnIndex="2" GridPane.rowIndex="8" />     
                <Button fx:id="submitForm" defaultButton="true" text="Submit" StackPane.alignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="9" />              
                                           
              </children>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>            
          </children>
        </VBox>
      </children>
    </StackPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</StackPane>